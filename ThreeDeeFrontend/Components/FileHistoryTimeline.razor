@using ThreeDeeApplication.Enums
<MudTimeline TimelineOrientation="@TimelineOrientation.Vertical" TimelinePosition="@TimelinePosition.Left">
    <TimeLineItem 
        State="@FileIncident.Negative" 
        Text="Druck mit Template XYZ fehlgeschlagen" 
        Timestamp="@DateTime.Now"/>
    <TimeLineItem 
        State="@FileIncident.Positive" 
        Text="Drucktemplate XYZ hinzugefügt" 
        Timestamp="@(DateTime.Now.Subtract(TimeSpan.FromMinutes(100)))"/>
    <TimeLineItem 
        State="@FileIncident.Neutral" 
        Text="Drucktemplate ABC geändert" 
        Timestamp="@(DateTime.Now.Subtract(TimeSpan.FromMinutes(1000)))"/>
    <TimeLineItem 
        State="@FileIncident.Positive" 
        Text="Druck mit Template ABC erfolgreich" 
        Timestamp="@(DateTime.Now.Subtract(TimeSpan.FromMinutes(4000)))"/>
    <TimeLineItem 
        State="@FileIncident.Positive" 
        Text="Drucktemplate ABC hinzugefügt" 
        Timestamp="@(DateTime.Now.Subtract(TimeSpan.FromMinutes(10000)))"/>
    <TimeLineItem 
        State="@FileIncident.Positive" 
        Text="Datei erstellt" 
        Timestamp="@(DateTime.Now.Subtract(TimeSpan.FromMinutes(14000)))"/>
</MudTimeline>